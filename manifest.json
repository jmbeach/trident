{
  "manifest_version": 2,
  "name": "Trident",
  "version": "0.1.2",
  "content_scripts": [
    {
      "js": [
        "js/content_script.js",
        "js/lib/youtube-iframe-api/youtube.iframe-api.js"
      ],
      "matches": [
        "*://*.pitchfork.com/*"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "./images/small.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "web_accessible_resources": [
    "config/config.json",
    "js/content_script.js.map",
    "js/web_accessible.js"
  ],
  "permissions": [
    "https://*/"
  ]
}